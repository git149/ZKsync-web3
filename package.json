{
    "name": "tsender-ui",
    "private": true,
    "version": "0.0.1",
    "description": "A 100% client-side UI for the TSender contract - Built by git149",
    "author": "git149 <git149@github.com>",
    "repository": {
        "type": "git",
        "url": "git@github.com:git149/Ts-Tsender-UI.git"
    },
    "homepage": "https://github.com/git149/Ts-Tsender-UI#readme",
    "license": "MIT",
    "keywords": [
        "tsender",
        "airdrop",
        "ethereum",
        "defi",
        "ui"
    ],
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "anvil": "anvil --load-state tsender-deployed.json",
        "coverage": "pnpm vitest run --coverage",
        "test:unit": "vitest",
        "test:cache": "pnpm synpress",
        "test:e2e": "pnpm exec playwright test",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx}\""
    },
    "dependencies": {
        "@rainbow-me/rainbowkit": "2.2.4",
        "@tanstack/react-query": "^5.80.7",
        "@wagmi/chains": "^1.8.0",
        "@wagmi/core": "^2.17.3",
        "next": "15.3.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-icons": "^5.5.0",
        "viem": "~2.31.0",
        "wagmi": "^2.15.6"
    },
    "devDependencies": {
        "@playwright/test": "1.48.2",
        "@synthetixio/synpress": "^4.0.10",
        "@types/node": "^20.12.10",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.0",
        "@vitest/coverage-v8": "^3.0.5",
        "@wagmi/cli": "latest",
        "bufferutil": "^4.0.8",
        "encoding": "^0.1.13",
        "jsdom": "^26.0.0",
        "lokijs": "^1.5.12",
        "pino-pretty": "^10.3.1",
        "playwright-core": "1.48.2",
        "prettier": "^3.5.0",
        "serve": "^14.2.4",
        "typescript": "^5.4.5",
        "utf-8-validate": "^5.0.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.5"
    }
}
